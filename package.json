{
  "name": "reactium",
  "version": "0.0.19",
  "description": "A framework for creating React + Redux apps using the domain driven design (DDD) paradigm.",
  "main": "index.js",
  "scripts": {
    "start": "node build/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm install && gulp --environment production",
    "local": "npm install && gulp --environment development",
    "clean": "gulp clean",
    "docker": "npm-run-all -s clean docker:*",
    "docker:build": "docker image build -t atomicreactor/reactium:$npm_package_version .",
    "docker:push": "docker push atomicreactor/reactium:$npm_package_version",
    "export-rods": "gulp --environment production rods",
    "deploy": "cf push"
  },
  "keywords": [
    "react",
    "redux",
    "framework",
    "javascript",
    "ddd"
  ],
  "author": "Cam Tullos <cam@tullos.ninja> (http://cam.tullos.ninja)",
  "license": "MIT",
  "engines": {
    "node": "9.2.1",
    "npm": ">=2.0.0"
  },
  "browser": {
    "parse/node": false
  },
